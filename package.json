{
  "name": "VuejsWithServer",
  "version": "0.0.1",
  "description": "simple app for test training",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --open --inline --hot --quiet --port 8082",
    "api": "nodemon --harmony ./server/server.js --port 4422",
    "api:test": "nodemon --harmony ./server/server.js --port 4422",
    "test:e2e": "./test.sh",
    "test:api": "NODE_ENV=test mocha --harmony --trace-deprecation --timeout 50000 $(find ./apitests/ -path '*.spec.*')",
    "update-webdriver": "node_modules\\.bin\\webdriver-manager update",
    "start-webdriver": "node_modules\\.bin\\webdriver-manager start",
    "nightwatch": "node_modules\\.bin\\nightwatch"
  },
  "author": "Potapov Dmytro",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^6.2.8",
    "babel-plugin-provide-modules": "^1.0.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.26.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "koa-logger": "^1.3.1",
    "minimist": "^1.2.0",
    "mocha": "^3.5.3",
    "nightwatch": "^0.9.15",
    "node-sass": "^4.6.1",
    "nodemon": "^1.12.1",
    "operadriver": "^0.2.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webdriver-manager": "^11.1.1",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.8.2"
  },
  "dependencies": {
    "dns-sync": "^0.1.3",
    "jsonwebtoken": "^7.4.3",
    "koa-bodyparser": "^2.5.0",
    "koa-cors": "0.0.16",
    "koa-router": "^7.3.0",
    "koa2": "^2.0.0-alpha.7",
    "lodash": "^4.17.4",
    "mongoose": "^4.13.2",
    "redux": "^3.7.2",
    "redux-vue": "^0.7.1",
    "rxjs": "^5.4.3",
    "then-redis": "^2.0.1",
    "vue": "^2.4.4",
    "vue-loader": "^12.2.2",
    "vue-router": "^2.7.0",
    "vue-template-compiler": "^2.4.4",
    "vuex": "^2.4.0"
  }
}
